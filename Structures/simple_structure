#include <stdio.h>
#include<string.h>

    struct person {
        char name[50];
        int citNo;
        float salary;
    }person1;
    
    int main(){
        strcpy(person1.name, "madhav zanwar");
        
        person1.citNo = 69;
        person1.salary = 200000;
        
        printf("Name : %s \n", person1.name);
        printf("Citizenship No. : %d \n", person1.citNo);
        printf("salary: %.2f",person1.salary);
    
  

    return 0;
}
